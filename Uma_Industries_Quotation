                int[] column_tblFooter = { 70, 30 };
                tblFooter.SetWidths(column_tblFooter);
                tblFooter.SpacingBefore = 0f;
                tblFooter.LockedWidth = true;
                tblFooter.HorizontalAlignment = Element.ALIGN_JUSTIFIED;

                if (lstTax[0].IGSTAmt > 0)
                {
                    tblIGST.AddCell(pdf.setCell("Total Tax(in words)                                                                                                     E. & O.E.", pdf.WhiteBaseColor, pdf.fnCalibri8, pdf.paddingOf4, 9, Element.ALIGN_LEFT, Element.ALIGN_MIDDLE, 13));
                    tblIGST.AddCell(pdf.setCell("Indian Rupees " + BAL.CommonMgmt.ConvertNumbertoWords((int)TaxAmount) + " Only", pdf.WhiteBaseColor, pdf.fnCalibriBold8, pdf.paddingOf4, 9, Element.ALIGN_LEFT, Element.ALIGN_MIDDLE, 12));
                    tblFooter.AddCell(pdf.setCell(tblIGST, pdf.WhiteBaseColor, pdf.fnCalibri8, 0, 2, Element.ALIGN_LEFT, Element.ALIGN_BOTTOM, 14));
                }
                else
                {
                    tblCSGST.AddCell(pdf.setCell("Total Tax(in words)                                                                                                   E. & O.E.", pdf.WhiteBaseColor, pdf.fnCalibri8, pdf.paddingOf4, 9, Element.ALIGN_LEFT, Element.ALIGN_MIDDLE, 13));
                    tblCSGST.AddCell(pdf.setCell("Indian Rupees " + BAL.CommonMgmt.ConvertNumbertoWords((int)TaxAmount) + " Only", pdf.WhiteBaseColor, pdf.fnCalibriBold8, pdf.paddingOf4, 9, Element.ALIGN_LEFT, Element.ALIGN_MIDDLE, 12));
                    tblFooter.AddCell(pdf.setCell(tblCSGST, pdf.WhiteBaseColor, pdf.fnCalibri8, 0, 2, Element.ALIGN_LEFT, Element.ALIGN_BOTTOM, 14));
                }
                tblFooter.AddCell(pdf.setCell(tblFootDetail, pdf.WhiteBaseColor, pdf.fnCalibri8, 0, 1, Element.ALIGN_LEFT, Element.ALIGN_BOTTOM, 15));
                tblFooter.AddCell(pdf.setCell(tblESignature1, pdf.WhiteBaseColor, pdf.fnCalibriBold8, 0, 1, Element.ALIGN_LEFT, Element.ALIGN_BOTTOM, 15));
