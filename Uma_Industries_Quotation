                        tblCSGST.AddCell(pdf.setCell(lstTax[i].SGSTPer.ToString(), pdf.WhiteBaseColor, pdf.fnCalibri9, pdf.paddingOf4, 1, Element.ALIGN_CENTER, Element.ALIGN_MIDDLE, 13));
