                Decimal TaxAmount = 0;
                TaxAmount = (lstQuot[0].SGSTAmt + lstQuot[0].CGSTAmt + lstQuot[0].IGSTAmt);
                tblCSGST.AddCell(pdf.setCell("Total ", pdf.WhiteBaseColor, pdf.fnCalibriBold9, pdf.paddingOf4, 1, Element.ALIGN_RIGHT, Element.ALIGN_MIDDLE, 13));
                tblCSGST.AddCell(pdf.setCell(lstQuot[0].BasicAmt.ToString(), pdf.WhiteBaseColor, pdf.fnCalibriBold9, pdf.paddingOf4, 1, Element.ALIGN_CENTER, Element.ALIGN_MIDDLE, 5));
                tblCSGST.AddCell(pdf.setCell(" ", pdf.WhiteBaseColor, pdf.fnCalibriBold9, pdf.paddingOf4, 1, Element.ALIGN_CENTER, Element.ALIGN_MIDDLE, 13));
                tblCSGST.AddCell(pdf.setCell(lstQuot[0].CGSTAmt.ToString(), pdf.WhiteBaseColor, pdf.fnCalibriBold9, pdf.paddingOf4, 1, Element.ALIGN_CENTER, Element.ALIGN_MIDDLE, 13));
                tblCSGST.AddCell(pdf.setCell(" ", pdf.WhiteBaseColor, pdf.fnCalibriBold9, pdf.paddingOf4, 1, Element.ALIGN_CENTER, Element.ALIGN_MIDDLE, 13));
                tblCSGST.AddCell(pdf.setCell(lstQuot[0].SGSTAmt.ToString(), pdf.WhiteBaseColor, pdf.fnCalibriBold9, pdf.paddingOf4, 1, Element.ALIGN_CENTER, Element.ALIGN_MIDDLE, 13));
                tblCSGST.AddCell(pdf.setCell(TaxAmount.ToString(), pdf.WhiteBaseColor, pdf.fnCalibriBold9, pdf.paddingOf4, 1, Element.ALIGN_CENTER, Element.ALIGN_MIDDLE, 13));


                tblCSGST.AddCell(pdf.setCell("Total Price", pdf.WhiteBaseColor, pdf.fnCalibriBold9, pdf.paddingOf4, 6, Element.ALIGN_CENTER, Element.ALIGN_MIDDLE, 13));
                tblCSGST.AddCell(pdf.setCell(lstQuot[0].NetAmt.ToString(), pdf.WhiteBaseColor, pdf.fnCalibriBold9, pdf.paddingOf4, 1, Element.ALIGN_CENTER, Element.ALIGN_MIDDLE, 13));
                #endregion
                // *-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-

                //  Defining : Terms & Condition
